<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LoveFinder.Views.RegisterPage">

    <StackLayout Padding="24">
        <Entry x:Name="FirstName" Placeholder="First Name"/>
        <Entry x:Name="LastName" Placeholder="Last Name"/>
        <Entry x:Name="Age" Placeholder="Age"/>
        <Entry x:Name="Gender" Placeholder="Gender"/>
        <Entry x:Name="SexualOrientation" Placeholder="Sexual Orientation"/>
        <Entry x:Name="Email" Placeholder="Email"/>
        <Entry x:Name="Password" Placeholder="Password"/>
        <Entry x:Name="PasswordCheck" Placeholder="Password Check"/>
    </StackLayout>
</ContentPage>